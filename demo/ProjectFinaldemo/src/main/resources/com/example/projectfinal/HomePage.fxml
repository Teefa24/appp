<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="500.0" prefWidth="800.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projectfinal.HomePageManager">
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <children>
      <AnchorPane prefHeight="490.0" prefWidth="804.0">
         <children>
            <Button fx:id="AddPostButton" layoutX="355.0" layoutY="149.0" mnemonicParsing="false" onAction="#AddpostButtonOnAction" prefHeight="107.0" prefWidth="66.0" text="Add Post" />
            <TextField fx:id="PostTextInput" layoutX="12.0" layoutY="149.0" prefHeight="107.0" prefWidth="330.0" />
            <ListView fx:id="ListViewPosts" layoutX="431.0" layoutY="2.0" prefHeight="327.0" prefWidth="330.0" />
            <Label fx:id="UserNameLabel" layoutX="138.0" layoutY="26.0" prefHeight="18.0" prefWidth="171.0" text="Welcome Ibrahim">
               <font>
                  <Font name="System Bold" size="16.0" />
               </font>
            </Label>
            <Button fx:id="AddFriendButton" layoutX="14.0" layoutY="311.0" mnemonicParsing="false" onAction="#AddFriendBtnOnAction" prefHeight="26.0" prefWidth="166.0" text="Add Friend" />
            <TextField fx:id="FriendNameTextInput" layoutX="14.0" layoutY="270.0" prefHeight="26.0" prefWidth="166.0" promptText="Friend User Name" />
            <ListView fx:id="ListViewFriendsList" layoutX="192.0" layoutY="270.0" prefHeight="191.0" prefWidth="230.0" />
            <TextField fx:id="CommentTextField" layoutX="431.0" layoutY="435.0" prefHeight="26.0" prefWidth="210.0" promptText="Comment" />
            <Button fx:id="LikeButton" layoutX="710.0" layoutY="435.0" mnemonicParsing="false" onAction="#AddLikeButtonOnAction" prefHeight="26.0" prefWidth="51.0" text="Like" />
            <Button fx:id="SendButton" layoutX="650.0" layoutY="435.0" mnemonicParsing="false" onAction="#AddCommentButtonOnAction" prefHeight="26.0" prefWidth="51.0" text="Send" />
            <Button fx:id="ChangeButton" layoutX="46.0" layoutY="104.0" mnemonicParsing="false" prefHeight="18.0" prefWidth="51.0" text="Change">
               <font>
                  <Font size="9.0" />
               </font>
            </Button>
            <Label fx:id="BioLabel" alignment="TOP_LEFT" layoutX="138.0" layoutY="61.0" prefHeight="71.0" prefWidth="287.0" text="Fake It Till You Make It">
               <font>
                  <Font name="System Bold" size="13.0" />
               </font>
            </Label>
            <ListView fx:id="ListViewComments" layoutX="431.0" layoutY="337.0" prefHeight="82.0" prefWidth="330.0" />
            <Label fx:id="LikesCountLabel" layoutX="730.0" layoutY="419.0" textAlignment="CENTER">
               <font>
                  <Font size="10.0" />
               </font>
            </Label>
            <ImageView fx:id="UserImage" fitHeight="82.0" fitWidth="100.0" layoutX="22.0" layoutY="11.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </AnchorPane>
   </children>
</VBox>
